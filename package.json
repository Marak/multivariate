{
  "name": "splitjs",
  "version": "0.0.1",
  "description": "A Javascript A/B test library",
  "main": "index.js",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "test": "jest --coverage",
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/connector",
      "<rootDir>/node_modules/"
    ]
  },
  "author": "Ufuk Kayserilioglu <ufuk@paralaus.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "jest": "^21.2.1",
    "redis": "^2.8.0"
  },
  "dependencies": {
    "uuid": "^3.1.0"
  }
}
